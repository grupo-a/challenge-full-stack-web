{
  "name": "challenge-full-stack-web",
  "version": "1.0.0",
  "description": "Challenge to +A Educação",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "commit-msg": "npx --no -- commitlint --edit ${1}"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run test",
      "git add"
    ]
  },
  "scripts": {
    "test": "npx jest /test/ --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyanlucasx/challenge-full-stack-web.git"
  },
  "author": "Yan Lucas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xyanlucasx/challenge-full-stack-web/issues"
  },
  "homepage": "https://github.com/xyanlucasx/challenge-full-stack-web#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3"
  }
}
